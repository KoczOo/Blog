@model IEnumerable<Post>

@{ 

}

<h2>Admin Panel</h2>

<div>
    <ul>
        @foreach (var post in Model)
        {
        <li>
            @post.Title
            <a asp-action="Edit" asp-controller="Panel" asp-route-id="@post.Id">
                Edit
            </a>
            <a asp-action="Remove" asp-controller="Panel" asp-route-id="@post.Id">
                Remove
            </a>
        </li>
        }

    </ul>
</div>