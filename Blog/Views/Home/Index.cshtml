@model IEnumerable<Post>

@{

}

<h2>Home Page</h2>
<img src="~/content/static/pexels-photo-2246822.jpeg" width="500" />
<div>
    <ul>
        @foreach (var post in Model)
        {
            <li>
                <a asp-action="Post" asp-controller="Home" asp-route-id="@post.Id">
                    @post.Title
                </a>
            </li>
        }

    </ul>
</div>  